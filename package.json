{
  "name": "@dotneet/notion-exporter",
  "version": "0.1.0",
  "description": "Export Notion pages to Markdown files",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "notion-exporter": "dist/index.js"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "test": "bun test",
    "build": "bun build ./src/index.ts --outdir ./dist --target node",
    "prepublishOnly": "bun run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "notion",
    "markdown",
    "export",
    "github"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@notionhq/client": "^2.2.13"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "bun-types": "latest",
    "typescript": "^5.0.0"
  }
}
